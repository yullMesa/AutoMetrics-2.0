me="SQL 1*">-- Creamos la tabla para el inventario técnico de piezas
CREATE TABLE IF NOT EXISTS diseno (
    id TEXT PRIMARY KEY,           -- Código alfanumérico único (ej: MOD-01)
    tabla TEXT NOT NULL,           -- Sistema al que pertenece (ej: Motor, Chasis)
    componente TEXT NOT NULL,      -- Nombre técnico oficial del elemento
    estado TEXT,                   -- Situación actual (Borrador, En Revisión, Aprobado)
    version TEXT                   -- Número de iteración para control de cambios (ej: v1.0)
);

-- Insertamos algunos datos iniciales para que el análisis de datos sea interesante
INSERT INTO diseno (id, tabla, componente, estado, version) VALUES 
('ENG-01', 'Motor', 'Bloque de Cilindros', 'Aprobado', 'v2.1'),
('CHA-05', 'Chasis', 'Travesaño Principal', 'En Revisión', 'v1.0'),
('SUS-02', 'Suspensión', 'Amortiguador Hidráulico', 'Borrador', 'v0.8'),
('ELE-10', 'Electrónica', 'Módulo de Control ECU', 'Aprobado', 'v3.2'),
('FRE-04', 'Frenos', 'Disco de Freno Cerámico', 'En Revisión', 'v1.1');</sql><current_tab id="0"/></tab_sql></sqlb_project>
