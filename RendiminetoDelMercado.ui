   <string notr="true">/* Estilo general de la barra de menú */
QMenuBar {
    background-color: #121212; /* Fondo gris casi negro */
    color: #00E5FF;             /* Texto cian neón */
    font-family: 'Segoe UI', sans-serif;
    font-size: 22px;            /* Tamaño de letra grande */
    font-weight: bold;
    border-bottom: 1px solid #00E5FF; /* Línea divisoria neón */
}

/* Estilo de cada ítem de la barra (Archivo, Operaciones, etc.) */
QMenuBar::item {
    background: transparent;
    padding: 8px 15px;
    margin: 2px;
    border-radius: 4px;
}

/* Efecto al pasar el mouse por los ítems principales */
QMenuBar::item:selected { 
    background-color: rgba(0, 229, 255, 0.2); /* Resplandor cian suave */
    color: #FFFFFF;                           /* Texto blanco al resaltar */
}

/* Estilo de los menús desplegables */
QMenu {
    background-color: #1a1a1a;
    color: #00E5FF;
    border: 1px solid #FF00FF;  /* Borde magenta neón */
    font-size: 13px;
    padding: 5px;
}

/* Estilo de las opciones dentro del menú desplegable */
QMenu::item {
    padding: 10px 30px 10px 20px;
    border-radius: 2px;
}

/* Efecto al seleccionar una opción del desplegable */
QMenu::item:selected {
    background-color: #FF00FF;  /* Fondo magenta neón al seleccionar */
    color: #000000;             /* Texto negro para contraste */
}

/* Estilo de la línea divisoria interna del menú */
QMenu::separator {
    height: 1px;
    background: #00E5FF;
    margin: 5px 10px;
}</string>
