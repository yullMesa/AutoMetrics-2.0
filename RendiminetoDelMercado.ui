    selection-color: #ffffff;
}

/* Estilo de los encabezados (Títulos de las columnas) */
QHeaderView::section {
    background-color: #1a1a1a;     /* Gris muy oscuro */
    color: #00E5FF;                /* Texto cian neón */
    padding: 6px;
    border: 1px solid #00E5FF;
    font-weight: bold;
    font-size: 14px;               /* Letra de encabezado un poco más grande */
}

/* Estilo de las barras de desplazamiento (Scrollbars) */
QScrollBar:vertical {
    border: none;
    background: #121212;
    width: 10px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background: #00E5FF;           /* Barra de arrastre cian */
    min-height: 20px;
    border-radius: 5px;
}

/* Estilo al seleccionar una celda */
QTableWidget::item:selected {
    border: 1px solid #FF00FF;     /* Borde magenta al seleccionar */
}</string>
