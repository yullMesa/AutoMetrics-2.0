            <property name="styleSheet">
             <string notr="true">/* Contenedor principal del visualizador de texto */
QTextBrowser {
    background-color: #121212;      /* Fondo oscuro consistente */
    color: #ffffff;                 /* Texto base en blanco */
    border: 2px solid #00E5FF;      /* Borde cian neón */
    border-radius: 5px;
    padding: 10px;
    font-family: 'Consolas', 'Monospace'; /* Fuente tipo código para estilo técnico */
    font-size: 13px;
    selection-background-color: #FF00FF; /* Resaltado magenta */
    selection-color: #000000;
}

/* Estilo de la barra de desplazamiento (Scrollbar) */
QScrollBar:vertical {
    border: none;
    background: #1a1a1a;
    width: 8px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background: #00E5FF;           /* Barra cian */
    min-height: 20px;
    border-radius: 4px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}</string>
