            <property name="styleSheet">
             <string notr="true">/* Contenedor principal del árbol */
QTreeWidget {
    background-color: #121212;      /* Fondo oscuro consistente */
    color: #ffffff;                 /* Texto de las ramas en blanco */
    border: 1px solid #00E5FF;      /* Borde cian neón */
    font-family: 'Segoe UI';
    font-size: 13px;
    outline: none;                  /* Elimina el borde punteado al hacer clic */
}

/* Estilo de los encabezados del árbol */
QHeaderView::section {
    background-color: #1a1a1a;
    color: #00E5FF;                 /* Títulos en cian */
    padding: 4px;
    border: 1px solid #00E5FF;
    font-weight: bold;
}

/* Estilo de los ítems (Ramas) */
QTreeWidget::item {
    padding: 5px;
    border-bottom: 1px solid rgba(0, 229, 255, 0.1); /* Línea divisoria sutil */
}

/* Efecto al pasar el mouse por una rama */
QTreeWidget::item:hover {
    background-color: rgba(0, 229, 255, 0.1);
    color: #00E5FF;
}

/* Efecto al seleccionar una rama */
QTreeWidget::item:selected {
    background-color: #FF00FF;      /* Resaltado magenta neón */
    color: #000000;                 /* Texto negro para legibilidad */
}

/* Estilo de las flechas de expansión (Indicates if branch is open/closed) */
QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    image: url(expand_icon.png);    /* Aquí podrías poner un icono cian */
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings {
    image: url(collapse_icon.png);
}</string>
