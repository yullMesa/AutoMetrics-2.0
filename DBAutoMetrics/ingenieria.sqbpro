CREATE TABLE IF NOT EXISTS gestion_requisitos (
    id_requisito INTEGER PRIMARY KEY AUTOINCREMENT,
    nombre_requisito TEXT NOT NULL,
    prioridad TEXT,
    estado TEXT,
    descripcion TEXT
);

-- 2. Tabla de Diseño y Modelado
CREATE TABLE IF NOT EXISTS diseño_modelado (
    id_modelo INTEGER PRIMARY KEY AUTOINCREMENT,
    nombre_pieza TEXT NOT NULL,
    version TEXT,
    software_usado TEXT,
    estado_diseno TEXT
);

-- 3. Tabla de Lista de Materiales (BOM)
CREATE TABLE IF NOT EXISTS lista_materiales (
    id_material INTEGER PRIMARY KEY AUTOINCREMENT,
    componente TEXT NOT NULL,
    cantidad INTEGER,
    proveedor TEXT,
    costo_unitario REAL
);

-- 4. Tabla de Control de Cambios
CREATE TABLE IF NOT EXISTS control_cambios (
    id_cambio INTEGER PRIMARY KEY AUTOINCREMENT,
    fecha_cambio DATE DEFAULT CURRENT_DATE,
    descripcion_cambio TEXT,
    autor TEXT,
    impacto TEXT
);

-- 5. Aseguramiento de Calidad (Ya tienes una, pero aquí está por si acaso)
CREATE TABLE IF NOT EXISTS aseguramiento_calidad (
    id_prueba INTEGER PRIMARY KEY AUTOINCREMENT,
    id_pieza INTEGER,
    persona TEXT,
    criterio TEXT,
    descripcion TEXT,
    fecha TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);</sql><current_tab id="0"/></tab_sql></sqlb_project>
