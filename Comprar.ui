    selection-background-color: #1A5276; /* Color azul ejecutivo al seleccionar texto */
}

/* EFECTO CUANDO EL USUARIO HACE CLIC (FOCUS) */
QLineEdit:focus {
    border: 1px solid #1A5276; /* Borde azul cobalto (el que elegimos para seriedad) */
    background-color: #252525; /* Se aclara ligeramente el fondo */
}

/* ESTILO CUANDO EL CAMPO ESTÁ DESACTIVADO (READ ONLY) */
QLineEdit:disabled {
    background-color: #121212;
    color: #555555;
    border: 1px solid #222222;
}

/* PLACEHOLDER (Texto de sugerencia) */
QLineEdit::placeholder {
    color: #666666;
    font-style: italic;
}</string>
