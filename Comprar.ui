   <string notr="true">/* Estilo de la barra principal */
QMenuBar {
    background-color: #121212;
    color: #ffffff;
    padding: 10px 5px; /* Más espacio vertical para que respire */
    font-size: 16px;    /* Letras más grandes y legibles */
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    border-bottom: 2px solid #333333;
}

/* Estilo de cada pestaña (Tienda, Dashboard, etc.) */
QMenuBar::item {
    background-color: transparent;
    padding: 12px 30px; /* Aumentamos el espacio horizontal (30px) */
    margin: 0px 5px;    /* Separación entre pestañas */
    border-radius: 6px;
    min-width: 120px;   /* Forzamos a que ocupen más ancho de base */
    text-align: center;
}

/* Efecto Hover (Pasar el mouse) */
QMenuBar::item:selected {
    background-color: #1e1e1e;
    color: #00ffcc;     /* Tu color cian neón característico */
    border-bottom: 2px solid #00ffcc; /* Subrayado neón */
}

/* Menú desplegable (Dropdown) */
QMenu {
    background-color: #1e1e1e;
    color: white;
    font-size: 15px;    /* Submenús un poco más grandes también */
    border: 1px solid #00ffcc;
    border-radius: 8px;
    padding: 10px;
}

/* Items dentro del menú desplegable */
QMenu::item {
    padding: 10px 40px 10px 30px; /* Mucho espacio horizontal en submenús */
    border-radius: 4px;
    margin-bottom: 2px;
}

/* Selección en submenús */
QMenu::item:selected {
    background-color: #00ffcc;
    color: #121212;
}

/* Separador más visible */
QMenu::separator {
    height: 2px;
    background: #333333;
    margin: 8px 10px;
}</string>
